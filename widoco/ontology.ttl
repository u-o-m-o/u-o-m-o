@prefix : <http://uomodhdk.github.io/Ontology/uomoprofile.owl#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://uomodhdk.github.io/Ontology/uomoprofile.owl> .

<http://uomodhdk.github.io/Ontology/uomoprofile.owl> rdf:type owl:Ontology ;
                                                      owl:imports <http://rdfs.org/sioc/ns#> ,
                                                                  <http://www.w3.org/2006/time#2016> ,
                                                                  <http://xmlns.com/foaf/0.1/> ;
                                                      <http://purl.org/dc/terms/description> "UOMO (User Oriented social Media Ontology) Profile is an ontology for describing the information about Social Media focusing on the User's Profile."@en ;
                                                      <http://purl.org/dc/terms/hasVersion> "0.1.1"@en ;
                                                      <http://purl.org/dc/terms/issued> "11/02/2019"@en ;
                                                      <http://purl.org/dc/terms/license> "https://creativecommons.org/licenses/by/4.0/"@en ;
                                                      <http://purl.org/dc/terms/modified> "16/02/2019"@en ;
                                                      <http://purl.org/dc/terms/publisher> "https://github.com/uomodhdk"@en ;
                                                      <http://purl.org/dc/terms/title> "UOMO (User Oriented social Media Ontology) Profile"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty ;
                                       rdfs:range xsd:string ;
                                       rdfs:domain _:genid90 .


###  http://purl.org/dc/terms/hasVersion
<http://purl.org/dc/terms/hasVersion> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
<http://purl.org/dc/terms/issued> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
<http://purl.org/dc/terms/modified> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
<http://purl.org/dc/terms/publisher> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/source
<http://purl.org/dc/terms/source> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://schema.org/additionalType
<http://schema.org/additionalType> rdf:type owl:AnnotationProperty ;
                                   <http://schema.org/domainIncludes> <http://schema.org/Thing> ;
                                   <http://schema.org/rangeIncludes> <http://schema.org/URL> ;
                                   <http://schema.org/sameAs> <https://schema.org/additionalType> ;
                                   rdfs:comment "An additional type for the item, typically used for adding more specific types from external vocabularies in microdata syntax. This is a relationship between something and a class that the thing is in. In RDFa syntax, it is better to use the native RDFa syntax - the 'typeof' attribute - for multiple types. Schema.org tools may have only weaker understanding of extra types, in particular those defined externally." ;
                                   rdfs:label "additionalType" ;
                                   rdfs:subPropertyOf rdf:type .


###  http://schema.org/category
<http://schema.org/category> rdf:type owl:AnnotationProperty .


###  http://schema.org/description
<http://schema.org/description> rdf:type owl:AnnotationProperty ;
                                <http://schema.org/domainIncludes> <http://schema.org/Thing> ;
                                <http://schema.org/rangeIncludes> <http://schema.org/Text> ;
                                <http://schema.org/sameAs> <https://schema.org/description> ;
                                rdfs:comment "A description of the item." ;
                                rdfs:label "description" .


###  http://schema.org/disambiguatingDescription
<http://schema.org/disambiguatingDescription> rdf:type owl:AnnotationProperty ;
                                              <http://schema.org/domainIncludes> <http://schema.org/Thing> ;
                                              <http://schema.org/rangeIncludes> <http://schema.org/Text> ;
                                              <http://schema.org/sameAs> <https://schema.org/disambiguatingDescription> ;
                                              rdfs:comment "A sub property of description. A short description of the item used to disambiguate from other, similar items. Information from other properties (in particular, name) may be necessary for the description to be useful for disambiguation." ;
                                              rdfs:label "disambiguatingDescription" ;
                                              rdfs:subPropertyOf <http://schema.org/description> .


###  http://schema.org/domainIncludes
<http://schema.org/domainIncludes> rdf:type owl:AnnotationProperty .


###  http://schema.org/globalLocationNumber
<http://schema.org/globalLocationNumber> rdf:type owl:AnnotationProperty ;
                                         <http://purl.org/dc/terms/source> <http://www.w3.org/wiki/WebSchemas/SchemaDotOrgSources#source_GoodRelationsTerms> ;
                                         <http://schema.org/domainIncludes> <http://schema.org/Organization> ,
                                                                            <http://schema.org/Person> ,
                                                                            <http://schema.org/Place> ;
                                         <http://schema.org/rangeIncludes> <http://schema.org/Text> ;
                                         <http://schema.org/sameAs> <https://schema.org/globalLocationNumber> ;
                                         rdfs:comment "The <a href=\"http://www.gs1.org/gln\">Global Location Number</a> (GLN, sometimes also referred to as International Location Number or ILN) of the respective organization, person, or place. The GLN is a 13-digit number used to identify parties and physical locations." ;
                                         rdfs:label "globalLocationNumber" ;
                                         rdfs:subPropertyOf <http://schema.org/identifier> .


###  http://schema.org/identifier
<http://schema.org/identifier> rdf:type owl:AnnotationProperty ;
                               <http://schema.org/domainIncludes> <http://schema.org/Thing> ;
                               <http://schema.org/rangeIncludes> <http://schema.org/PropertyValue> ,
                                                                 <http://schema.org/Text> ,
                                                                 <http://schema.org/URL> ;
                               <http://schema.org/sameAs> <https://schema.org/identifier> ;
                               rdfs:comment "The identifier property represents any kind of identifier for any kind of <a class=\"localLink\" href=\"http://schema.org/Thing\">Thing</a>, such as ISBNs, GTIN codes, UUIDs etc. Schema.org provides dedicated properties for representing many of these, either as textual strings or as URL (URI) links. See <a href=\"/docs/datamodel.html#identifierBg\">background notes</a> for more details." ;
                               rdfs:label "identifier" .


###  http://schema.org/image
<http://schema.org/image> rdf:type owl:AnnotationProperty ;
                          <http://schema.org/domainIncludes> <http://schema.org/Thing> ;
                          <http://schema.org/rangeIncludes> <http://schema.org/ImageObject> ,
                                                            <http://schema.org/URL> ;
                          <http://schema.org/sameAs> <https://schema.org/image> ;
                          rdfs:comment "An image of the item. This can be a <a class=\"localLink\" href=\"http://schema.org/URL\">URL</a> or a fully described <a class=\"localLink\" href=\"http://schema.org/ImageObject\">ImageObject</a>." ;
                          rdfs:label "image" .


###  http://schema.org/inverseOf
<http://schema.org/inverseOf> rdf:type owl:AnnotationProperty .


###  http://schema.org/isPartOf
<http://schema.org/isPartOf> rdf:type owl:AnnotationProperty .


###  http://schema.org/logo
<http://schema.org/logo> rdf:type owl:AnnotationProperty ;
                         <http://purl.org/dc/terms/source> <http://www.w3.org/wiki/WebSchemas/SchemaDotOrgSources#source_GoodRelationsTerms> ;
                         <http://schema.org/domainIncludes> <http://schema.org/Brand> ,
                                                            <http://schema.org/Organization> ,
                                                            <http://schema.org/Place> ,
                                                            <http://schema.org/Product> ,
                                                            <http://schema.org/Service> ;
                         <http://schema.org/rangeIncludes> <http://schema.org/ImageObject> ,
                                                           <http://schema.org/URL> ;
                         <http://schema.org/sameAs> <https://schema.org/logo> ;
                         rdfs:comment "An associated logo." ;
                         rdfs:label "logo" ;
                         rdfs:subPropertyOf <http://schema.org/image> .


###  http://schema.org/name
<http://schema.org/name> rdf:type owl:AnnotationProperty ;
                         <http://schema.org/domainIncludes> <http://schema.org/Thing> ;
                         <http://schema.org/rangeIncludes> <http://schema.org/Text> ;
                         <http://schema.org/sameAs> <https://schema.org/name> ;
                         rdfs:comment "The name of the item." ;
                         rdfs:label "name" ;
                         rdfs:subPropertyOf <rdfs:label> .


###  http://schema.org/photo
<http://schema.org/photo> rdf:type owl:AnnotationProperty ;
                          <http://schema.org/domainIncludes> <http://schema.org/Place> ;
                          <http://schema.org/rangeIncludes> <http://schema.org/ImageObject> ,
                                                            <http://schema.org/Photograph> ;
                          <http://schema.org/sameAs> <https://schema.org/photo> ;
                          rdfs:comment "A photograph of this place." ;
                          rdfs:label "photo" ;
                          rdfs:subPropertyOf <http://schema.org/image> .


###  http://schema.org/rangeIncludes
<http://schema.org/rangeIncludes> rdf:type owl:AnnotationProperty .


###  http://schema.org/sameAs
<http://schema.org/sameAs> rdf:type owl:AnnotationProperty ;
                           <http://schema.org/domainIncludes> <http://schema.org/Thing> ;
                           <http://schema.org/rangeIncludes> <http://schema.org/URL> ;
                           <http://schema.org/sameAs> <https://schema.org/sameAs> ;
                           rdfs:comment "URL of a reference Web page that unambiguously indicates the item's identity. E.g. the URL of the item's Wikipedia page, Wikidata entry, or official website." ;
                           rdfs:label "sameAs" .


###  http://schema.org/supersededBy
<http://schema.org/supersededBy> rdf:type owl:AnnotationProperty .


###  http://www.w3.org/1999/02/22-rdf-syntax-ns#type
rdf:type rdf:type owl:AnnotationProperty .


###  rdfs:label
<rdfs:label> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://rdfs.org/sioc/ns#container_of
<http://rdfs.org/sioc/ns#container_of> owl:equivalentProperty :is_container_of .


###  http://rdfs.org/sioc/ns#has_container
<http://rdfs.org/sioc/ns#has_container> owl:equivalentProperty :has_container .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#contains
:contains rdf:type owl:ObjectProperty ;
          rdfs:domain :Content_Container ;
          rdfs:range [ rdf:type owl:Class ;
                       owl:unionOf ( :Content
                                     :Content_Container
                                   )
                     ] ;
          rdfs:label "contains" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#created_by
:created_by rdf:type owl:ObjectProperty ;
            owl:inverseOf :creates ;
            rdfs:domain :Profile ;
            rdfs:range <http://rdfs.org/sioc/ns#UserAccount> ,
                       :User ;
            rdfs:label "created by" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#creates
:creates rdf:type owl:ObjectProperty ;
         rdfs:domain <http://rdfs.org/sioc/ns#UserAccount> ,
                     :User ;
         rdfs:range :Profile ;
         rdfs:label "creates" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_achievements
:has_achievements rdf:type owl:ObjectProperty ;
                  owl:inverseOf :is_an_achievement_of ;
                  rdfs:domain :Profile ;
                  rdfs:range :Achievement ;
                  rdfs:label "has achievements" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_biographical_info
:has_biographical_info rdf:type owl:ObjectProperty ;
                       owl:inverseOf :is_a_biographical_info_of ;
                       rdfs:domain :Profile ;
                       rdfs:range :Biographical_info ;
                       rdfs:label "has biographical info" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_color_palette
:has_color_palette rdf:type owl:ObjectProperty ;
                   owl:inverseOf :is_color_palette_of ;
                   rdfs:domain :Profile ;
                   rdfs:range :Color_Palette ;
                   rdfs:label "has color palette" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_contact_info
:has_contact_info rdf:type owl:ObjectProperty ;
                  owl:inverseOf :is_contact_info_of ;
                  rdfs:domain :Profile ;
                  rdfs:range :Contact_info ;
                  rdfs:label "has contact info" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_container
:has_container rdf:type owl:ObjectProperty ;
               owl:inverseOf :is_container_of ;
               rdfs:domain :Profile ;
               rdfs:range <http://rdfs.org/sioc/ns#Container> ;
               rdfs:label "has container" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_displaying_order
:has_displaying_order rdf:type owl:ObjectProperty ;
                      rdfs:domain <http://rdfs.org/sioc/ns#Container> ;
                      rdfs:range :Displaying_Order ;
                      rdfs:label "has displaying order" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_input_content_box
:has_input_content_box rdf:type owl:ObjectProperty ;
                       owl:inverseOf :is_input_content_box_of ;
                       rdfs:domain :Profile ;
                       rdfs:range :Input_content_box ;
                       rdfs:label "has input content box" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_inscription_date
:has_inscription_date rdf:type owl:ObjectProperty ;
                      rdfs:domain :Profile ;
                      rdfs:range :Inscription_Date ;
                      rdfs:label "has inscription date" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_main_media
:has_main_media rdf:type owl:ObjectProperty ;
                owl:inverseOf :is_main_media_of ;
                rdfs:domain :Profile ;
                rdfs:range :Main_Media ;
                rdfs:label "has main media" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_media
:has_media rdf:type owl:ObjectProperty ;
           owl:inverseOf :is_media_of ;
           rdfs:domain :Background ;
           rdfs:range :Media ;
           rdfs:label "has media" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_nickname
:has_nickname rdf:type owl:ObjectProperty ;
              rdfs:domain <http://rdfs.org/sioc/ns#UserAccount> ;
              rdfs:range [ rdf:type owl:Restriction ;
                           owl:onProperty :has_nickname ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onClass :Nickname
                         ] ;
              rdfs:label "has nickname" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_place
:has_place rdf:type owl:ObjectProperty ;
           owl:inverseOf :is_place_of ;
           rdfs:domain [ rdf:type owl:Class ;
                         owl:unionOf ( :Actual_Place
                                       :Birth
                                       :Experience
                                     )
                       ] ;
           rdfs:range <http://schema.org/Place> ;
           rdfs:label "has place" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_privacy
:has_privacy rdf:type owl:ObjectProperty ;
             rdfs:domain :Social_Media_Data ;
             rdfs:range :Privacy ;
             rdfs:label "has privacy" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#has_stats
:has_stats rdf:type owl:ObjectProperty ;
           owl:inverseOf :is_a_stat_of ;
           rdfs:domain :Profile ;
           rdfs:range :Stats ;
           rdfs:label "has stats" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_a_biographical_info_of
:is_a_biographical_info_of rdf:type owl:ObjectProperty ;
                           rdfs:domain :Biographical_info ;
                           rdfs:range :Profile ;
                           rdfs:label "is a biographical info of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_a_stat_of
:is_a_stat_of rdf:type owl:ObjectProperty ;
              rdfs:domain :Stats ;
              rdfs:range :Profile ;
              rdfs:label "is a stat of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_an_achievement_of
:is_an_achievement_of rdf:type owl:ObjectProperty ;
                      rdfs:domain :Achievement ;
                      rdfs:range :Profile ;
                      rdfs:label "is an achievement of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_color_palette_of
:is_color_palette_of rdf:type owl:ObjectProperty ;
                     rdfs:domain :Color_Palette ;
                     rdfs:range :Profile ;
                     rdfs:label "is color palette of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_contact_info_of
:is_contact_info_of rdf:type owl:ObjectProperty ;
                    rdfs:domain :Contact_info ;
                    rdfs:range :Profile ;
                    rdfs:label "is contact info of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_contained_in
:is_contained_in rdf:type owl:ObjectProperty ;
                 rdfs:domain :Content ;
                 rdfs:range :Content_Container ;
                 rdfs:label "is contained in" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_container_of
:is_container_of rdf:type owl:ObjectProperty ;
                 rdfs:domain <http://rdfs.org/sioc/ns#Container> ;
                 rdfs:range :Profile ;
                 rdfs:label "is container of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_displaying_order_of
:is_displaying_order_of rdf:type owl:ObjectProperty ;
                        rdfs:domain :Displaying_Order ;
                        rdfs:range <http://rdfs.org/sioc/ns#Container> ;
                        rdfs:label "is displaying order of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_held_at
:is_held_at rdf:type owl:ObjectProperty ;
            rdfs:domain [ rdf:type owl:Class ;
                          owl:unionOf ( :Educational_Experience
                                        :Working_Experience
                                      )
                        ] ;
            rdfs:range <http://xmlns.com/foaf/0.1/Organization> ;
            rdfs:label "is held at" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_input_content_box_of
:is_input_content_box_of rdf:type owl:ObjectProperty ;
                         rdfs:domain :Input_content_box ;
                         rdfs:range :Profile ;
                         rdfs:label "is input content box of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_main_media_of
:is_main_media_of rdf:type owl:ObjectProperty ;
                  rdfs:domain :Main_Media ;
                  rdfs:range :Profile ;
                  rdfs:label "is main media of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_media_of
:is_media_of rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:domain :Media ;
             rdfs:range :Background ;
             rdfs:label "is media of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_nickname_of
:is_nickname_of rdf:type owl:ObjectProperty ;
                rdfs:domain :Nickname ;
                rdfs:range <http://rdfs.org/sioc/ns#UserAccount> ;
                rdfs:label "is nickname of" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_place_of
:is_place_of rdf:type owl:ObjectProperty ;
             rdfs:domain <http://schema.org/Place> ;
             rdfs:range [ rdf:type owl:Class ;
                          owl:unionOf ( :Actual_Place
                                        :Birth
                                        :Experience
                                      )
                        ] ;
             rdfs:label "is place of" .


###  http://www.w3.org/2006/time#hasTime
<http://www.w3.org/2006/time#hasTime> rdfs:domain [ rdf:type owl:Class ;
                                                    owl:unionOf ( :Birth
                                                                  :Experience
                                                                  :Inscription_Date
                                                                  :Name_Day
                                                                )
                                                  ] .


#################################################################
#    Data properties
#################################################################

###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#allowed_emotes
:allowed_emotes rdf:type owl:DatatypeProperty ;
                rdfs:domain :Chat ;
                rdfs:range xsd:string ;
                rdfs:comment "It is a particular property of Twitch because the user can ban some emotes on her/his chat."@en ;
                rdfs:label "allowed emotes" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#answer
:answer rdf:type owl:DatatypeProperty ;
        rdfs:domain :Trivia ;
        rdfs:range xsd:string ;
        rdfs:comment "It is intended as the answer to a certain question in the \"Trivia\" section of a profile"@en ;
        rdfs:label "answer" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#area_of_study
:area_of_study rdf:type owl:DatatypeProperty ;
               rdfs:domain :Educational_Experience ;
               rdfs:range xsd:string ;
               rdfs:comment "It is intended both as the type of University or school general study program"@en ;
               rdfs:label "area of study" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#bio
:bio rdf:type owl:DatatypeProperty ;
     rdfs:subPropertyOf <http://purl.org/dc/terms/description> ;
     rdfs:domain :Profile ;
     rdfs:range xsd:string ;
     rdfs:label "bio" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#category
:category rdf:type owl:DatatypeProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Highlighted_Media
                                      :Personal_Experience
                                    )
                      ] ;
          rdfs:range xsd:string ;
          rdfs:comment "It is used to categorize a certain experience or highlighted media"@en ;
          rdfs:label "category" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#certification
:certification rdf:type owl:DatatypeProperty ;
               rdfs:domain :Educational_Experience ;
               rdfs:range xsd:anyURI ;
               rdfs:comment "It is intended for every kind of educational certificates (School diploma or Proof)"@en ;
               rdfs:label "certification" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#color
:color rdf:type owl:DatatypeProperty ;
       rdfs:domain :Color_Palette ;
       rdfs:range rdfs:Literal ;
       rdfs:label "color" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#evaluation
:evaluation rdf:type owl:DatatypeProperty ;
            rdfs:domain :Educational_Experience ;
            rdfs:range [ rdf:type rdfs:Datatype ;
                         owl:unionOf ( xsd:nonNegativeInteger
                                       xsd:string
                                     )
                       ] ;
            rdfs:comment "It is intended as the grade obtained at the end of a particular educational experience"@en ;
            rdfs:label "evaluation" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#extracurricular_activities
:extracurricular_activities rdf:type owl:DatatypeProperty ;
                            rdfs:domain :Educational_Experience ;
                            rdfs:range xsd:string ;
                            rdfs:comment "It is intended as the activities done during an educational experience that are not directly linked to the courses followed or grades obtained. (Example: Theatre Club, Nomination as the president of students or others...)"@en ;
                            rdfs:label "extracurrricular activities" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#favorite_quotes
:favorite_quotes rdf:type owl:DatatypeProperty ;
                 rdfs:subPropertyOf :other_biographic_data ;
                 rdfs:comment "It is intended a particular biographic information that displays the favorite quotes of a user, all in string format"@en ;
                 rdfs:label "favorite quotes" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#field
:field rdf:type owl:DatatypeProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Working_Experience
                                   <http://xmlns.com/foaf/0.1/Organization>
                                 )
                   ] ;
       rdfs:range xsd:string ;
       rdfs:comment "The field of the work done either by an organization or by the user in her/his working experience"@en ;
       rdfs:label "field" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#full_name
:full_name rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf :other_biographic_data ;
           rdfs:range rdfs:Literal ;
           rdfs:label "full name" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_automatic
:is_automatic rdf:type owl:DatatypeProperty ;
              rdfs:domain :Content ;
              rdfs:range xsd:boolean ;
              rdfs:comment "With automatic it is meant that the container is generated automatically by inference of the Social Media itself (such as the \"Comments\" and \"Post\" containers of Reddit) and that the user cannot directly modify it (like you can in Facebook)" ;
              rdfs:label "is automatic" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#is_new_to_old
:is_new_to_old rdf:type owl:DatatypeProperty ;
               rdfs:domain :Chronological_Order ;
               rdfs:range xsd:boolean ;
               rdfs:comment "It is intended as the property of a chronological order because usually in social media the chronological order is from new to old, meanwhile usually it should start from the old and go to the new."@en ;
               rdfs:label "is new to old" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#label
:label rdf:type owl:DatatypeProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Link_to_other_Social_Media
                                   :Skill
                                   :Working_Experience
                                 )
                   ] ;
       rdfs:range xsd:string ;
       rdfs:comment "It is used to give a name to some social media elements"@en ;
       rdfs:label "label" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#language
:language rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :other_biographic_data ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( :Biographical_info
                                      :Language_Competency
                                    )
                      ] ;
          rdfs:range xsd:string ;
          rdfs:label "language" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#level
:level rdf:type owl:DatatypeProperty ;
       rdfs:domain :Language_Competency ;
       rdfs:range xsd:string ;
       rdfs:comment "Particular data property applied to language competency that describes the competency level of that particular language"@en ;
       rdfs:label "level" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#link
:link rdf:type owl:DatatypeProperty ;
      rdfs:domain :Hypertext ;
      rdfs:range xsd:anyURI ;
      rdfs:comment "Different from uri because it's not always considered as an identificator"@en ;
      rdfs:label "link" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#max_character_number
:max_character_number rdf:type owl:DatatypeProperty ;
                      rdfs:domain :Input_content_box ;
                      rdfs:range xsd:nonNegativeInteger ;
                      rdfs:comment "Some input content box have a cap of characters that you can type into them"@en ;
                      rdfs:label "max character number" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#other_biographic_data
:other_biographic_data rdf:type owl:DatatypeProperty ;
                       rdfs:domain :Biographical_info ;
                       rdfs:range xsd:string ;
                       rdfs:label "other biographic data" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#political_orientation
:political_orientation rdf:type owl:DatatypeProperty ;
                       rdfs:subPropertyOf :other_biographic_data ;
                       rdfs:label "political orientation" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#question
:question rdf:type owl:DatatypeProperty ;
          rdfs:domain :Trivia ;
          rdfs:range xsd:string ;
          rdfs:comment "It is intended as the question in the \"Trivia\" section of a profile"@en ;
          rdfs:label "question" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#religion
:religion rdf:type owl:DatatypeProperty ;
          rdfs:subPropertyOf :other_biographic_data ;
          rdfs:label "religion" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#sexual_orientation
:sexual_orientation rdf:type owl:DatatypeProperty ;
                    rdfs:subPropertyOf :other_biographic_data ;
                    rdfs:label "sexual orientation" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#shareable
:shareable rdf:type owl:DatatypeProperty ;
           rdfs:domain :Social_Media_Data ;
           rdfs:range xsd:boolean ;
           rdfs:comment "It applies to every social media data to specify if it is shareable or not."@en ;
           rdfs:label "shareable" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#social_media_icon
:social_media_icon rdf:type owl:DatatypeProperty ;
                   rdfs:domain :Link_to_other_Social_Media ;
                   rdfs:range xsd:boolean ;
                   rdfs:comment "It is used to specify, in the context of linking different social medias in a profile, if the social medias linked are represented also by an icon."@en ;
                   rdfs:label "social media icon" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#title
:title rdf:type owl:DatatypeProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Highlighted_Media
                                   :Personal_Experience
                                 )
                   ] ;
       rdfs:range xsd:string ;
       rdfs:comment "It is used for all the medias or personal experiences that may have a title"@en ;
       rdfs:label "title" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#uri
:uri rdf:type owl:DatatypeProperty ;
     rdfs:domain :Profile ;
     rdfs:range xsd:anyURI ;
     rdfs:label "uri" .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#value
:value rdf:type owl:DatatypeProperty ;
       rdfs:domain [ rdf:type owl:Class ;
                     owl:unionOf ( :Contact_info
                                   :Stats
                                 )
                   ] ;
       rdfs:range [ rdf:type rdfs:Datatype ;
                    owl:unionOf ( xsd:nonNegativeInteger
                                  xsd:string
                                )
                  ] ;
       rdfs:comment "It can contain the general value of stats or contact info"@en ;
       rdfs:label "value" .


###  http://xmlns.com/foaf/0.1/age
<http://xmlns.com/foaf/0.1/age> rdfs:subPropertyOf :other_biographic_data .


###  http://xmlns.com/foaf/0.1/family_name
<http://xmlns.com/foaf/0.1/family_name> rdfs:subPropertyOf :full_name .


###  http://xmlns.com/foaf/0.1/firstName
<http://xmlns.com/foaf/0.1/firstName> rdfs:subPropertyOf :full_name .


###  http://xmlns.com/foaf/0.1/gender
<http://xmlns.com/foaf/0.1/gender> rdfs:subPropertyOf :other_biographic_data .


###  http://xmlns.com/foaf/0.1/nick
<http://xmlns.com/foaf/0.1/nick> rdfs:domain :Nickname ;
                                 rdfs:range rdfs:Literal .


###  http://xmlns.com/foaf/0.1/surname
<http://xmlns.com/foaf/0.1/surname> rdfs:subPropertyOf :full_name .


#################################################################
#    Classes
#################################################################

###  http://rdfs.org/sioc/ns#Container
<http://rdfs.org/sioc/ns#Container> rdfs:subClassOf :Social_Media_Data ;
                                    owl:disjointWith :Content ,
                                                     :Profile .


###  http://rdfs.org/sioc/ns#UserAccount
<http://rdfs.org/sioc/ns#UserAccount> rdfs:subClassOf [ rdf:type owl:Restriction ;
                                                        owl:onProperty :creates ;
                                                        owl:someValuesFrom :Profile
                                                      ] ,
                                                      [ rdf:type owl:Restriction ;
                                                        owl:onProperty :has_nickname ;
                                                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                        owl:onClass :Nickname
                                                      ] ;
                                      owl:disjointWith :Profile .


###  http://schema.org/Accommodation
<http://schema.org/Accommodation> rdf:type owl:Class ;
                                  rdfs:subClassOf <http://schema.org/Place> .


###  http://schema.org/AdministrativeArea
<http://schema.org/AdministrativeArea> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://schema.org/Place> .


###  http://schema.org/CivicStructure
<http://schema.org/CivicStructure> rdf:type owl:Class ;
                                   rdfs:subClassOf <http://schema.org/Place> .


###  http://schema.org/Landform
<http://schema.org/Landform> rdf:type owl:Class ;
                             rdfs:subClassOf <http://schema.org/Place> .


###  http://schema.org/LandmarksOrHistoricalBuildings
<http://schema.org/LandmarksOrHistoricalBuildings> rdf:type owl:Class ;
                                                   rdfs:subClassOf <http://schema.org/Place> .


###  http://schema.org/LocalBusiness
<http://schema.org/LocalBusiness> rdf:type owl:Class ;
                                  rdfs:subClassOf <http://schema.org/Place> .


###  http://schema.org/Place
<http://schema.org/Place> rdf:type owl:Class ;
                          rdfs:subClassOf <http://schema.org/Thing> ,
                                          [ rdf:type owl:Restriction ;
                                            owl:onProperty :is_place_of ;
                                            owl:someValuesFrom [ rdf:type owl:Class ;
                                                                 owl:unionOf ( :Actual_Place
                                                                               :Birth
                                                                               :Experience
                                                                             )
                                                               ]
                                          ] ;
                          owl:disjointWith :Actual_Place ,
                                           :Birth ,
                                           :Experience ;
                          <http://schema.org/sameAs> <https://schema.org/Place> ;
                          rdfs:comment "Entities that have a somewhat fixed, physical extension." ;
                          rdfs:label "Place" .


###  http://schema.org/Residence
<http://schema.org/Residence> rdf:type owl:Class ;
                              rdfs:subClassOf <http://schema.org/Place> .


###  http://schema.org/Thing
<http://schema.org/Thing> rdf:type owl:Class ;
                          <http://schema.org/sameAs> <https://schema.org/Thing> ;
                          rdfs:comment "The most generic type of item." ;
                          rdfs:label "Thing" .


###  http://schema.org/TouristAttraction
<http://schema.org/TouristAttraction> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://schema.org/Place> .


###  http://schema.org/TouristDestination
<http://schema.org/TouristDestination> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://schema.org/Place> .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Achievement
:Achievement rdf:type owl:Class ;
             rdfs:subClassOf :Social_Media_Data ;
             owl:disjointWith :Profile ;
             rdfs:comment "Some socials have some goals that can be reached, and once it happens the achievement appears on a user profile"@en ;
             rdfs:label "Achievement"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Actual_Place
:Actual_Place rdf:type owl:Class ;
              rdfs:subClassOf :Biographical_info ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :has_place ;
                                owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                owl:onClass <http://schema.org/Place>
                              ] ;
              rdfs:comment "It is intended as a personalized features of some profiles that give the user the possibility to specifiy a place either with a link (uri) internal to the social media or just by typing it (string)"@en ;
              rdfs:label "Actual Place"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Address
:Address rdf:type owl:Class ;
         rdfs:subClassOf :Contact_info ;
         rdfs:label "Address"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Algorithmic_Order
:Algorithmic_Order rdf:type owl:Class ;
                   rdfs:subClassOf :Displaying_Order .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Alphabetical_Order
:Alphabetical_Order rdf:type owl:Class ;
                    rdfs:subClassOf :Displaying_Order .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Background
:Background rdf:type owl:Class ;
            rdfs:subClassOf :Biographical_info ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :has_media ;
                              owl:someValuesFrom :Media
                            ] ;
            owl:disjointWith :Media ;
            rdfs:comment "Also History"@en ,
                         "It is intended as the experiences of the user and also the skills of the user"@en ;
            rdfs:label "Background"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Background_Media
:Background_Media rdf:type owl:Class ;
                  rdfs:subClassOf :Main_Media ;
                  rdfs:label "Background Media"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Biographical_info
:Biographical_info rdf:type owl:Class ;
                   rdfs:subClassOf :Social_Media_Data ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty <http://www.w3.org/2006/time#hasTime> ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass <http://www.w3.org/2006/time#TemporalEntity>
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :other_biographic_data ;
                                     owl:someValuesFrom xsd:string
                                   ] ;
                   owl:disjointWith :Profile ;
                   rdfs:label "Biographical Info"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Birth
:Birth rdf:type owl:Class ;
       rdfs:subClassOf :Biographical_info ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :has_place ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass <http://schema.org/Place>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty <http://www.w3.org/2006/time#hasTime> ;
                         owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                         owl:onClass <http://www.w3.org/2006/time#TemporalEntity>
                       ] ;
       owl:disjointWith <http://www.w3.org/2006/time#TemporalEntity> ;
       rdfs:label "Birth"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Chat
:Chat rdf:type owl:Class ;
      rdfs:subClassOf :Input_content_box ,
                      [ rdf:type owl:Restriction ;
                        owl:onProperty :allowed_emotes ;
                        owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                        owl:onDataRange xsd:string
                      ] ;
      rdfs:comment "In this particular kind of input box the communication is shared between possibly multiple users"@en ;
      rdfs:label "Chat"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Chronological_Order
:Chronological_Order rdf:type owl:Class ;
                     rdfs:subClassOf :Displaying_Order ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :is_new_to_old ;
                                       owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onDataRange xsd:boolean
                                     ] ;
                     rdfs:label "Chronological Order"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Color_Palette
:Color_Palette rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :color ;
                                 owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                 owl:onDataRange xsd:string
                               ] ;
               owl:disjointWith :Profile ;
               rdfs:comment "Color palette is intended as the range of colors that can be applied to general elements of the profile such as texts, containers or the background."@en ;
               rdfs:isDefinedBy <http://uomodhdk.github.io/Ontology/uomoprofile.owl#UOMO:UOMO:UOMO> ;
               rdfs:label "Color Palette"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Contact_info
:Contact_info rdf:type owl:Class ;
              rdfs:subClassOf :Social_Media_Data ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :value ;
                                owl:someValuesFrom [ rdf:type rdfs:Datatype ;
                                                     owl:unionOf ( xsd:nonNegativeInteger
                                                                   xsd:string
                                                                 )
                                                   ]
                              ] ;
              owl:disjointWith :Profile ;
              rdfs:comment "It is intended as a section of a profile that gives some form of communication (not direct in the social) between the visitor of the profile and the person behind the user"@en ;
              rdfs:label "Contact Info"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Container
:Container rdf:type owl:Class ;
           rdfs:subClassOf :Social_Media_Data ;
           rdfs:comment "A container is intended as a section of a profile that contains other elements such has media, hyperlinks, tags"@en ;
           owl:deprecated "true"^^xsd:boolean .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Content
:Content rdf:type owl:Class ;
         rdfs:subClassOf :Social_Media_Data ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :is_automatic ;
                           owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:boolean
                         ] ;
         rdfs:label "Content"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Content_Container
:Content_Container rdf:type owl:Class ;
                   rdfs:subClassOf <http://rdfs.org/sioc/ns#Container> ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :contains ;
                                     owl:someValuesFrom :Content
                                   ] ;
                   rdfs:label "Content Container"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Displaying_Order
:Displaying_Order rdf:type owl:Class ;
                  owl:disjointWith :Profile ;
                  rdfs:comment "The order through which the items in a container are displayed"@en ;
                  rdfs:isDefinedBy <http://uomodhdk.github.io/Ontology/uomoprofile.owl#UOMO:UOMO:UOMO> ;
                  rdfs:label "Displaying Order"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Educational_Experience
:Educational_Experience rdf:type owl:Class ;
                        rdfs:subClassOf :Experience ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :is_held_at ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onClass <http://xmlns.com/foaf/0.1/Organization>
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :area_of_study ;
                                          owl:someValuesFrom xsd:string
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :extracurricular_activities ;
                                          owl:someValuesFrom xsd:string
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :certification ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onDataRange xsd:anyURI
                                        ] ,
                                        [ rdf:type owl:Restriction ;
                                          owl:onProperty :evaluation ;
                                          owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                          owl:onDataRange [ rdf:type rdfs:Datatype ;
                                                            owl:unionOf ( xsd:nonNegativeInteger
                                                                          xsd:string
                                                                        )
                                                          ]
                                        ] ;
                        owl:disjointWith <http://xmlns.com/foaf/0.1/Organization> ;
                        rdfs:label "Educational Experience"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Email
:Email rdf:type owl:Class ;
       rdfs:subClassOf :Contact_info ;
       rdfs:label "Email"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Experience
:Experience rdf:type owl:Class ;
            rdfs:subClassOf :Background ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty :has_place ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass <http://schema.org/Place>
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty <http://www.w3.org/2006/time#hasTime> ;
                              owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                              owl:onClass <http://www.w3.org/2006/time#TemporalEntity>
                            ] ;
            owl:disjointWith <http://www.w3.org/2006/time#TemporalEntity> ;
            rdfs:label "Experience"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Fax
:Fax rdf:type owl:Class ;
     rdfs:subClassOf :Contact_info ;
     rdfs:label "Fax"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Followers
:Followers rdf:type owl:Class ;
           rdfs:subClassOf :Network_Container ;
           rdfs:label "Followers"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Following
:Following rdf:type owl:Class ;
           rdfs:subClassOf :Network_Container ;
           rdfs:label "Following"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Form
:Form rdf:type owl:Class ;
      rdfs:subClassOf :Input_content_box ;
      rdfs:label "Form"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Friends
:Friends rdf:type owl:Class ;
         rdfs:subClassOf :Network_Container ;
         rdfs:label "Friends"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Group
:Group rdf:type owl:Class ;
       rdfs:subClassOf :Network_Container ;
       rdfs:label "Group"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Highlighted_Media
:Highlighted_Media rdf:type owl:Class ;
                   rdfs:subClassOf :Main_Media ;
                   rdfs:comment "With Highlighted Media it is meant the main media that is displayed in a part of the profile which is not the background or the profile pic. As an example we have the main video played when you enter a Youtube channel or the Twitch stream in a Twitch profile. Note that both these Social Media have a Profile Picture."@en ;
                   rdfs:label "Highlighted Media"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Hypertext
:Hypertext rdf:type owl:Class ;
           rdfs:subClassOf :Contact_info ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty :link ;
                             owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                             owl:onDataRange xsd:anyURI
                           ] ;
           rdfs:label "Hypertext"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Input_content_box
:Input_content_box rdf:type owl:Class ;
                   rdfs:subClassOf :Social_Media_Data ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :is_input_content_box_of ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onClass :Profile
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty :max_character_number ;
                                     owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                     owl:onDataRange xsd:nonNegativeInteger
                                   ] ;
                   owl:disjointWith :Profile ;
                   rdfs:comment "It is intended as a section of a profile that gives some form of communication (direct in the social) between the visitor of the profile and the user"@en ;
                   rdfs:label "Input Content Box"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Inscription_Date
:Inscription_Date rdf:type owl:Class ;
                  rdfs:subClassOf :Social_Media_Data ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty <http://www.w3.org/2006/time#hasTime> ;
                                    owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                    owl:onClass <http://www.w3.org/2006/time#TemporalEntity>
                                  ] ;
                  rdfs:comment "It is intended as the information that some social display to see the date of the creation of the profile"@en ;
                  rdfs:label "Inscription Date"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Interest_Container
:Interest_Container rdf:type owl:Class ;
                    rdfs:subClassOf <http://rdfs.org/sioc/ns#Container> ;
                    rdfs:comment "It is intended as a container that aggregates some tags that represent particular interests of the user"@en ;
                    rdfs:label "Interest Container"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Karma
:Karma rdf:type owl:Class ;
       rdfs:subClassOf :Stats ;
       rdfs:comment "Particular stats that is displayed on Reddit"@en ;
       rdfs:label "Karma"@en .


###  http://uomodhdk.github.io/Ontology/uomoprofile.owl#Language_Competency
:Language_Competency rdf:type owl:Class ;
                     rdfs:subClassOf :Skill ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :language ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                       owl:onDataRange xsd:string
                                     ] ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :level ;
                                       owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
